<!DOCTYPE html>
<html lang="fr">

<head>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-123731981-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }

        gtag('js', new Date());
        gtag('config', 'UA-123731981-1');
    </script>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1"> {% block meta_tag_desc %}
    <meta name="description" content="Vendez rapidement et gratuitement vos annonces partout au Sénégal !"> {% endblock %} {% block meta_tag_keywords %}
    <meta name="keywords" content="annonces, vente en ligne, achat en ligne, annonces dakar, annonces senegal, vente dakar, vente saint-louis, annonces saint-louis"> {% endblock %}
    <title>{% block title %}SENADS &ndash; La meilleure plate-forme d'annonces au Sénégal !{% endblock %}</title>

    <!-- CSS -->
    {% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('css/font-awesome.min.cs') }}s">
    <link rel="stylesheet" href="{{ asset('css/icofont.cs') }}s">
    <link rel="stylesheet" href="{{ asset('css/owl.carousel.css') }}">
    <link rel="stylesheet" href="{{ asset('css/slidr.css') }}">
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    <link id="preset" rel="stylesheet" href="{{ asset('css/presets/preset1.css') }}">
    <link rel="stylesheet" href="{{ asset('css/responsive.css') }}">
    <!-- font -->
    <link href='https://fonts.googleapis.com/css?family=Ubuntu:400,500,700,300' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Signika+Negative:400,300,600,700' rel='stylesheet' type='text/css'> {% endblock stylesheet %}
    <!-- icons -->
    <link rel="icon" href="{{ asset('favicon.ico') }}">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <!-- header -->
    <header id="header" class="clearfix">
        <!-- navbar -->
        <nav class="navbar navbar-default">
            <div class="container">
                <!-- navbar-header -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					</button>
                    <a class="navbar-brand" href="{{ asset('homepage') }}"><img class="img-responsive" src="{{ asset('images/senads-logo.png') }}" alt="Logo"></a>
                </div>
                <!-- /navbar-header -->

                <div class="navbar-left">
                    <div class="collapse navbar-collapse" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="{{ path('homepage')   }}">Accueil</a></li>
                            <li><a href="{{ path('ads_home')   }}">Annonces</a></li>
                            <li><a href="{{ path('shops_home')   }}">Boutiques</a></li>
                            <li class="dropdown"><a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown">Pages <span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="{{ path('ads_new') }}">Publier une annonce</a></li>
                                    <li><a href="{{ path('shop_new') }}">Créer une boutique</a></li>
                                    <li><a href="{{ path('login') }}">Se connecter</a></li>
                                    <li><a href="{{ path('register') }}">S'inscrire</a></li>
                                    <li><a href="{{ path('about') }}">À propos de {{ site_name }}</a></li>
                                    <li><a href="{{ path('contact') }}">Contact</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>

                <!-- nav-right -->
                <div class="nav-right">
                    <!-- sign-in -->
                    <ul class="sign-in">
                        <li><i class="fa fa-user"></i></li>
                        <li><a href="{{ path('login') }}">Se connecter</a></li>
                        <li><a href="{{ path('register') }}">S'inscrire</a></li>
                    </ul>
                    <!-- sign-in -->

                    <a href="{{ path('ads_new') }}" class="btn">Publier une annonce</a>
                </div>
                <!-- nav-right -->
            </div>
            <!-- container -->
        </nav>
        <!-- navbar -->
    </header>
    <!-- header -->

    <!-- main -->
    <section id="main" class="clearfix category-page">
        <div class="container">
            {% block body %}{% endblock %}
        </div>
    </section>
    <!-- main -->

    <!-- footer -->
    <footer id="footer" class="clearfix">
        <!-- footer-top -->
        <section class="footer-top clearfix">
            <div class="container">
                <div class="row">
                    <!-- footer-widget -->
                    <div class="col-sm-3">
                        <div class="footer-widget">
                            <h3>Liens rapides</h3>
                            <ul>
                                <li><a href="{{ path('about') }}">À propos de {{ site_name }}</a></li>
                                <li><a href="{{ path('contact') }}">Contactez-nous</a></li>
                                <li><a href="{{ path('help_faq') }}">Aide / FAQ</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- footer-widget -->

                    <!-- footer-widget -->
                    <div class="col-sm-3">
                        <div class="footer-widget">
                            <h3>Vendre rapidement</h3>
                            <ul>
                                <li><a href="{{ path('sell_fast') }}">Comment vendre rapidement</a></li>
                                <li><a href="{{ path('banner_ads') }}">Bannière publicitaire</a></li>
                                <li><a href="{{ path('promote_ads') }}">Promouvoir votre annonce</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- footer-widget -->

                    <!-- footer-widget -->
                    <div class="col-sm-3">
                        <div class="footer-widget social-widget">
                            <h3>Suivez-nous sur</h3>
                            <ul>
                                <li><a href="https://facebook.com/senads221"><i class="fa fa-facebook-official"></i>Facebook</a></li>
                                <li><a href="https://instagram.com/sen.ads"><i class="fa fa-instagram"></i>Instagram</a></li>
                                <li><a href="https://twitter.com/senads"><i class="fa fa-twitter-square"></i>Twitter</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- footer-widget -->

                    <!-- footer-widget -->
                    <div class="col-sm-3">
                        <div class="footer-widget news-letter">
                            <h3>Newsletter</h3>
                            <p>Recevez rapidement les annonces sur votre boite mail</p>
                            <!-- form -->
                            <form action="{{ path('regsiter_newsletter') }}">
                                <input type="email" class="form-control" placeholder="Votre adresse email">
                                <button type="submit" class="btn btn-primary">S'inscrire</button>
                            </form>
                            <!-- form -->
                        </div>
                    </div>
                    <!-- footer-widget -->
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </section>
        <!-- footer-top -->


        <div class="footer-bottom clearfix text-center">
            <div class="container">
                <p>Copyright &copy; 2018 &mdash; {{ site_name }}.</p>
            </div>
        </div>
        <!-- footer-bottom -->
    </footer>
    <!-- footer -->

    <!--/Preset Style Chooser-->
    <div class="style-chooser">
        <div class="style-chooser-inner">
            <a href="#" class="toggler"><i class="fa fa-life-ring fa-spin"></i></a>
            <h4>Presets</h4>
            <ul class="preset-list clearfix">
                <li class="preset1 active" data-preset="1">
                    <a href="#" data-color="preset1"></a>
                </li>
                <li class="preset2" data-preset="2">
                    <a href="#" data-color="preset2"></a>
                </li>
                <li class="preset3" data-preset="3">
                    <a href="#" data-color="preset3"></a>
                </li>
                <li class="preset4" data-preset="4">
                    <a href="#" data-color="preset4"></a>
                </li>
            </ul>
        </div>
    </div>
    <!--/End:Preset Style Chooser-->

    {% block javascript %}
    <!-- JS -->
    <script src="js/jquery.min.js"></script>
    <script src="js/modernizr.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/scrollup.min.js"></script>
    <script src="js/custom.js"></script>
    <script src="js/switcher.js"></script>
    {% endblock javascript %}
</body>

</html>